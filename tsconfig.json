{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "module": "CommonJS",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@machines/*": [
        "src/machines/*"
      ],
      "@services/*": [
        "src/services/*"
      ]
    }
  }
}